{
  "name": "where-was-i",
  "version": "0.1.0",
  "description": "A plugin for your page to display an interactive history of your users session",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shootsville/where-was-i"
  },
  "keywords": [
    "userplugin",
    "history",
    "user",
    "journey",
    "typescript"
  ],
  "author": "Gustaf Eriksson Segerdorf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shootsville/where-was-i/issues"
  },
  "homepage": "https://github.com/shootsville/where-was-i#readme",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^28.3.0",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "^5.4.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "html2canvas": "^1.4.1",
    "url-change-event": "^0.1.7"
  }
}
